{
  "name": "tech-task-app",
  "version": "1.0.0",
  "description": "Viable Data Tech Task App",
  "main": "index.js",
  "scripts": {
    "build": "export $(xargs < ./.env) && webpack --mode production",
    "start": "export $(xargs < ./.env) && webpack serve --mode development",
    "start:prod": "export $(xargs < ./.env) && webpack serve --mode production",
    "lint": "eslint \"**/*.{js,jsx}\" -f table",
    "lint:fix": "eslint \"./src/**/*.{js,jsx}\" --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "cypress:run:local": "cypress run --config-file cypress/config/local.config.js",
    "cypress:open:local": "cypress open --config-file cypress/config/local.config.js",
    "cypress:run:dev": "cypress run --config-file cypress/config/dev.config.js",
    "cypress:open:dev": "cypress open --config-file cypress/config/dev.config.js",
    "cypress:run:staging": "cypress run --config-file cypress/config/staging.config.js",
    "cypress:open:staging": "cypress open --config-file cypress/config/staging.config.js",
    "cypress:report": "node cucumber-html-report.js",
    "cypress:dev:regression": "cypress run --config-file cypress/config/dev.config.js --env TAGS=@regression",
    "cypress:staging:regression": "cypress run --config-file cypress/config/staging.config.js --env TAGS=@regression"
  },
  "repository": {
    "type": "git",
    "url": "this-apps-repo"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "http://issues-url"
  },
  "homepage": "https://readme-url",
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "axios-mock-adapter": "^1.21.2",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-formatter-table": "^7.32.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "mini-css-extract-plugin": "^2.7.2",
    "sass": "^1.54.9",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "dayjs": "^1.11.7",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-router-dom": "^6.4.0"
  }
}
